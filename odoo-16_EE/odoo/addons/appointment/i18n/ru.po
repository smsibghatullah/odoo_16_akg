"                            <t t-else=\"\">\n"
"                                <span style=\"margin-left:5px\">Вы</span>\n"
"                            </t>\n"
"                        </li>\n"
"                    </ul></li>\n"
"                    <li t-if=\"object.videocall_location\">\n"
